program CelsiusAFahrenheit;
    var 
    temperaturaCelsius: real;
    temperaturaFahrenheit: real;

begin

    writeln('Temperatura en CÂ°: ');
    readln(temperaturaCelsius);
        
    temperaturaFahrenheit := (temperaturaCelsius * 1.8 + 32);
    
    writeln('La temperatura es: ', temperaturaFahrenheit:0:2,'Â°F');
    
end.
